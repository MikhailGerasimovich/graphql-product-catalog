{
  "name": "graphql-product-catalog",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "set NODE_ENV=development && nest start",
    "start:dev": "set NODE_ENV=development && nest start --watch",
    "start:docker": "export NODE_ENV=docker && nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/graphql-product-catalog/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/graphql-product-catalog/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:auth:create": "npx typeorm migration:create ./apps/authorization/src/migrations/init-schema",
    "migration:catalogs-command:create": "npx typeorm migration:create ./apps/catalogs/src/migrations/init-command-schema",
    "migration:catalogs-query:create": "npx typeorm migration:create ./apps/catalogs/src/migrations/init-query-schema",
    "migration:baskets:create": "npx typeorm migration:create ./apps/baskets/src/migrations/init-schema",
    "migration:orders:create": "npx typeorm migration:create ./apps/orders/src/migrations/init-schema",
    "migration:auth:run": "export NODE_ENV=docker && npm run typeorm -- --dataSource=apps/authorization/src/config/typeorm-cli.config.ts migration:run",
    "migration:catalogs-command:run": "export NODE_ENV=docker && npm run typeorm -- --dataSource=apps/catalogs/src/config/typeorm-command-cli.config.ts migration:run",
    "migration:catalogs-query:run": "export NODE_ENV=docker && npm run typeorm -- --dataSource=apps/catalogs/src/config/typeorm-query-cli.config.ts migration:run",
    "migration:baskets:run": "export NODE_ENV=docker && npm run typeorm -- --dataSource=apps/baskets/src/config/typeorm-cli.config.ts migration:run",
    "migration:orders:run": "export NODE_ENV=docker && npm run typeorm -- --dataSource=apps/orders/src/config/typeorm-cli.config.ts migration:run",
    "auth:docker": "npm run migration:auth:run; npm run start:docker authorization",
    "catalog:docker": "npm run migration:catalogs-command:run; npm run migration:catalogs-query:run; npm run start:docker catalogs",
    "basket:docker": "npm run migration:baskets:run; npm run start:docker baskets",
    "order:docker": "npm run migration:orders:run; npm run start:docker orders"
  },
  "dependencies": {
    "@apollo/gateway": "^2.5.5",
    "@apollo/server": "^4.9.4",
    "@apollo/subgraph": "^2.5.5",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.2.7",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/typeorm": "^10.0.0",
    "@types/cookie-parser": "^1.4.5",
    "@types/express-session": "^1.17.9",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "apollo-server-cache-redis": "^3.3.1",
    "apollo-server-express": "^3.12.1",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "nest-winston": "^1.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stripe": "^14.1.0",
    "ts-morph": "^19.0.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
